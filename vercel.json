{
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/test/aq10",
      "dest": "/test/aq10"
    },
    {
      "src": "/test/aq50", 
      "dest": "/test/aq50"
    },
    {
      "src": "/admin",
      "dest": "/admin"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}